// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

model Measure{
  id String @id
  type String
  value Int
  hasConfirmed Boolean
  datetime DateTime
  customerId   String
  imageUrl      String
}